<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lew.gateway.dao.UserDao">
    <!-- 根据用户id查询 -->
    <select id="findByUserId" resultType="com.lew.gateway.model.User">
        SELECT * FROM user WHERE id = #{id} limit 1
    </select>
</mapper>
